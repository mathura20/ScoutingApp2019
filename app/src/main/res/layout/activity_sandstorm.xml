        android:layout_height="24dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:text="@string/sHPickedPrompt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sCPickedPrompt" />

    <TextView
        android:id="@+id/sHRocket2Prompt"
        android:layout_width="185dp"
        android:layout_height="24dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:text="@string/sHRocket2Prompt"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sHRocket1Prompt" />

    <com.example.aayushscoutingapp.Counter
        android:id="@+id/r2Cargo"
        android:layout_width="169dp"
        android:layout_height="27dp"
        android:layout_marginTop="288dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toEndOf="@+id/sCCargoPrompt"
        app:layout_constraintTop_toTopOf="parent" />

    <com.example.aayushscoutingapp.Counter
